{
  "name": "ultimate-frisbee-manager-backend",
  "version": "1.0.0",
  "description": "Backend pour l'application de gestion d'exercices d'ultimate frisbee",
  "main": "server.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "prestart": "prisma generate",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed:destructive": "cross-env SEED_DESTRUCTIVE=true prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "deploy": "npm run db:deploy && npm start",
    "export:dryrun": "cross-env-shell \"node backend/scripts/export-ufm.mjs --baseUrl=${API} --email=${ADMIN_EMAIL} --password=${ADMIN_PASSWORD} --dryRun\"",
    "export:run": "cross-env-shell \"node backend/scripts/export-ufm.mjs --baseUrl=${API} --token=${TOKEN} --outDir=backend/exports\""
  },
  "prisma": {
    "seed": "node prisma/seed.js && node prisma/seed-minimal-content.js"
  },
  "keywords": [
    "ultimate",
    "frisbee",
    "exercices",
    "coach",
    "sport"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@supabase/supabase-js": "^2.93.2",
    "@ufm/shared": "file:../shared",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "gray-matter": "^4.0.3",
    "helmet": "^7.1.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "pino-http": "^9.0.0",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/pg": "^8.10.7",
    "cross-env": "^10.1.0",
    "jest": "^30.1.3",
    "nodemon": "^3.0.1",
    "prisma": "5.22.0",
    "supabase": "^2.40.7",
    "supertest": "^7.1.4"
  }
}

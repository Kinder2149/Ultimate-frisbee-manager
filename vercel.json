{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist/ultimate-frisbee-manager"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.[^/]+)$",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}